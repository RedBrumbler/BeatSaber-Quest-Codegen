{
  "sharedDir": "include",
  "dependenciesDir": "extern",
  "info": {
    "name": "bs-cordl",
    "id": "bs-cordl",
    "version": "0.0.1",
    "url": "https://github.com/sc2ad/BeatSaber-Quest-Codegen",
    "additionalData": {
      "headersOnly": true,
      "compileOptions": {
        "includePaths": ["include"],
        "cppFeatures": [],
        "cppFlags": ["-DNEED_UNSAFE_CSHARP", "-fdeclspec", "-DUNITY_2021"]
      }
    }
  },
  "dependencies": [
    {
      "id": "libil2cpp",
      "versionRange": "^0.3.0",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.0.0",
      "additionalData": {
        "useRelease": true
      }
    }
  ],
  "workspace": {
    "scripts": {
      "build": [
        "deno run --allow-read --allow-write --allow-run ./scripts/deno/compile_pch.ts"
      ]
    }
  }
}
